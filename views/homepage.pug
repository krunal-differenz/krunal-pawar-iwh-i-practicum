doctype html
html
  head
    title= title 
    meta(name="viewport", content="width=device-width, initial-scale=1")
    link(rel="stylesheet", href="/css/style.css")
  body
    h1 Actor List
    table
      thead
        tr
          th Name
          th Favorite Movie of Actor 
          th Age
      tbody
        each contact in data
          tr
            td #{contact.properties.actor_name}
            td #{contact.properties.actor_movie_name}
            td #{contact.properties.actor_age}
    br        
    a(href="/update-cobj") Add to this table
